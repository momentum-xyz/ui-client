{
  "name": "@momentum-xyz/ui-kit",
  "version": "0.1.0",
  "license": "GPL-3.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "yarn clean && tsc --listEmittedFiles && yarn copy-files",
    "start": "yarn clean && yarn copy-files && tsc -w",
    "copy-files": "copyfiles -u 1 src/**/*.svg dist/",
    "svg-sprite:build": "node scripts/buildSvgSprite.js",
    "svg-sprite:type": "node scripts/generateIconsType.js"
  },
  "dependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "styled-components": "^5.3.3",
    "lodash-es": "^4.17.21",
    "polished": "^4.1.4",
    "react-dropzone": "^12.0.4",
    "i18next": "^21.6.12",
    "@headlessui/react": "^1.2.0",
    "react-router-dom": "^5.2.0",
    "svg-sprite": "^1.5.4",
    "@momentum-xyz/core": "^0.1.0"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "require-context": "^1.1.0",
    "rimraf": "^3.0.2"
  }
}
