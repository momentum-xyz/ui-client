{
  "name": "@momentum-xyz/ui-kit",
  "version": "0.1.3",
  "license": "GPL-3.0",
  "repository": "https://github.com/momentum-xyz/ui-client",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "yarn clean && tsc --listEmittedFiles && yarn copy-files",
    "start": "yarn copy-files && tsc -w",
    "copy-files": "copyfiles -u 1 src/**/*.svg dist/ && copyfiles -u 1 src/**/*.css dist/",
    "svg-sprite:build": "node scripts/buildSvgSprite.js",
    "svg-sprite:type": "node scripts/generateIconsType.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@headlessui/react": "^1.2.0",
    "classnames": "^2.3.2",
    "i18next": "^21.6.12",
    "lodash-es": "^4.17.21",
    "polished": "^4.1.4",
    "rc-tooltip": "^5.1.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-dropzone": "^12.0.4",
    "react-router-dom": "^5.2.0",
    "react-use-error-boundary": "^3.0.0",
    "styled-components": "^5.3.3",
    "svg-sprite": "^1.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@react-theming/storybook-addon": "^1.1.10",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "babel-loader": "^8.3.0",
    "copyfiles": "^2.4.1",
    "require-context": "^1.1.0",
    "rimraf": "^3.0.2"
  }
}
