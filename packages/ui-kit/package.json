{
  "name": "@momentum-xyz/ui-kit",
  "version": "0.1.4",
  "license": "GPL-3.0",
  "repository": "https://github.com/momentum-xyz/ui-client",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "yarn clean && tsc --listEmittedFiles && yarn copy-files",
    "start": "yarn copy-files && tsc -w",
    "copy-files": "yarn copy-files:static && yarn copy-files:fonts",
    "copy-files:static": "copyfiles -u 1 src/static/**/*.** dist && copyfiles -u 1 src/**/*.svg dist/",
    "copy-files:fonts": "copyfiles -u 1 src/static/**/*.ttf src/static/**/*.woff src/static/**/*.woff2 dist",
    "svg-sprite:build": "node scripts/buildSvgSprite.js",
    "svg-sprite:type": "node scripts/generateIconsType.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook --output-dir=dist --disable-telemetry"
  },
  "dependencies": {
    "@headlessui/react": "^1.2.0",
    "@momentum-xyz/core": "^0.1.2",
    "@types/react-linkify": "^1.0.1",
    "classnames": "^2.3.2",
    "lodash-es": "^4.17.21",
    "polished": "^4.1.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.42.1",
    "react-imask": "^6.4.3",
    "react-linkify": "^1.0.0-alpha",
    "react-multi-carousel": "^2.8.3",
    "react-number-format": "^5.1.4",
    "react-player": "^2.12.0",
    "react-router-dom": "^6.7.0",
    "react-select": "^5.7.0",
    "react-share": "^4.4.1",
    "react-timer-hook": "^3.0.6",
    "react-use-error-boundary": "^3.0.0",
    "styled-components": "^5.3.6",
    "svg-sprite": "^1.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@react-theming/storybook-addon": "^1.1.10",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-backgrounds": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addons": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.16",
    "@types/react-select": "^5.0.1",
    "babel-loader": "^8.3.0",
    "copyfiles": "^2.4.1",
    "require-context": "^1.1.0",
    "rimraf": "^3.0.2"
  }
}
