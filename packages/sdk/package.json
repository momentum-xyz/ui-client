{
  "name": "@momentum-xyz/sdk",
  "version": "0.1.5",
  "license": "GPL-3.0",
  "homepage": "https://momentum.xyz",
  "repository": "https://github.com/momentum-xyz/ui-client",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "copy-files": "copyfiles -u 1 bin/*.js dist/bin && copyfiles craco.config.js dist/ && copyfiles public/* dist/",
    "clean": "rimraf dist/",
    "build": "yarn clean && yarn copy-files && tsc --listEmittedFiles",
    "start": "yarn copy-files && tsc -w"
  },
  "dependencies": {
    "@craco/craco": "^6.1.1",
    "@momentum-xyz/ui-kit-storybook": "^0.1.4",
    "react-router-dom": "^6.6.2"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "bin": {
    "momentum-plugin": "./bin/momentum-plugin.js"
  }
}
